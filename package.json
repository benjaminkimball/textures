{
  "private": true,
  "name": "textures",
  "version": "1.0.0",
  "description": "Recreating textures.js (https://riccardoscalco.it/textures/) in React.",
  "scripts": {
    "build": "parcel build src/index.html --out-dir docs --public-url /textures",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint src/**/*.js",
    "publish": "rimraf docs && npm run lint && npm run build",
    "start": "parcel src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:benjaminkimball/textures.git"
  },
  "author": "Benjamin Kimball <hello@benjaminkimball.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjaminkimball/textures/issues"
  },
  "homepage": "https://github.com/benjaminkimball/textures#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
